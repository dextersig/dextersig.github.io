<!DOCTYPE html>
<html lang="en">
{% include "head.njk" %}

  <body>

    {% include "header.njk" %}
    <main>
      {{ content | safe}}

      <ul class="postlist">
        {% for post in collections.posts | reverse %}
         <li>
          <i>[{{post.date | readableDate}}]</i> <a href="{{ post.url }}">{{ post.data.title }}</a>
         </li>
         <br>
        {% endfor %}
        </ul>
    </main>

    {% include "webring.njk" %} 

    {% include "footer.njk" %}  
  </body>
</html>